{  "manifest_version": 3,  "name": "Gemini Prompt Chains",  "version": "1.0.0",  "description": "Free, unlimited prompt chains for Google Gemini. Save, edit, import/export, and auto-queue prompts.",  "action": {    "default_popup": "popup.html",    "default_title": "Gemini Prompt Chains"  },  "permissions": ["storage", "scripting", "tabs", "activeTab"],  "host_permissions": [    "https://gemini.google.com/*",    "https://gemini.google.com/app*",    "https://chat.google.com/*"  ],  "background": {    "service_worker": "background.js",    "type": "module"  },  "content_scripts": [    {      "matches": [        "https://gemini.google.com/*",        "https://gemini.google.com/app*",        "https://chat.google.com/*"      ],      "js": ["contentScript.js"],      "run_at": "document_idle"    }  ]}